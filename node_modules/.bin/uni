#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3.0.0-alpha-3081220230802001_postcss@8.4.23_vite@4.0.3_vue@3.2.45/node_modules/@dcloudio/vite-plugin-uni/bin/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3.0.0-alpha-3081220230802001_postcss@8.4.23_vite@4.0.3_vue@3.2.45/node_modules/@dcloudio/vite-plugin-uni/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3.0.0-alpha-3081220230802001_postcss@8.4.23_vite@4.0.3_vue@3.2.45/node_modules/@dcloudio/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3.0.0-alpha-3081220230802001_postcss@8.4.23_vite@4.0.3_vue@3.2.45/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3.0.0-alpha-3081220230802001_postcss@8.4.23_vite@4.0.3_vue@3.2.45/node_modules/@dcloudio/vite-plugin-uni/bin/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3.0.0-alpha-3081220230802001_postcss@8.4.23_vite@4.0.3_vue@3.2.45/node_modules/@dcloudio/vite-plugin-uni/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3.0.0-alpha-3081220230802001_postcss@8.4.23_vite@4.0.3_vue@3.2.45/node_modules/@dcloudio/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/@dcloudio+vite-plugin-uni@3.0.0-alpha-3081220230802001_postcss@8.4.23_vite@4.0.3_vue@3.2.45/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@dcloudio/vite-plugin-uni/bin/uni.js" "$@"
else
  exec node  "$basedir/../@dcloudio/vite-plugin-uni/bin/uni.js" "$@"
fi
