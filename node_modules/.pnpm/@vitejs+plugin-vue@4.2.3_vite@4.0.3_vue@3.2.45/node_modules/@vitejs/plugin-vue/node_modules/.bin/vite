#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/vite@4.0.3_@types+node@18.11.10_sass@1.56.1_terser@5.16.0/node_modules/vite/bin/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/vite@4.0.3_@types+node@18.11.10_sass@1.56.1_terser@5.16.0/node_modules/vite/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/vite@4.0.3_@types+node@18.11.10_sass@1.56.1_terser@5.16.0/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/vite@4.0.3_@types+node@18.11.10_sass@1.56.1_terser@5.16.0/node_modules/vite/bin/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/vite@4.0.3_@types+node@18.11.10_sass@1.56.1_terser@5.16.0/node_modules/vite/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/vite@4.0.3_@types+node@18.11.10_sass@1.56.1_terser@5.16.0/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@4.0.3_@types+node@18.11.10_sass@1.56.1_terser@5.16.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@4.0.3_@types+node@18.11.10_sass@1.56.1_terser@5.16.0/node_modules/vite/bin/vite.js" "$@"
fi
