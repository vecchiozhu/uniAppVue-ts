#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/registry.npmmirror.com+jest@27.0.4/node_modules/jest/bin/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/registry.npmmirror.com+jest@27.0.4/node_modules/jest/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/registry.npmmirror.com+jest@27.0.4/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/registry.npmmirror.com+jest@27.0.4/node_modules/jest/bin/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/registry.npmmirror.com+jest@27.0.4/node_modules/jest/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/registry.npmmirror.com+jest@27.0.4/node_modules:/Users/mac/vecchioZ/uniapp-demo/heima-shop/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jest/bin/jest.js" "$@"
else
  exec node  "$basedir/../jest/bin/jest.js" "$@"
fi
